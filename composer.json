{
    "name": "geekyants/sharedo",
    "description": "Share Popup in Laravel Apps",
    "keywords": [
        "share",
        "inertiajs",
        "vue",
        "laravel",
        "geekyants"
    ],
    "homepage": "https://github.com/GeekyAnts/laravel-inertia-sharedo",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Cyrus Passi",
            "email": "cyruspassi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "laravel/ui": "^3.2"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Geekyants\\Sharedo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geekyants\\Sharedo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Geekyants\\Sharedo\\SharedoServiceProvider"
            ],
            "aliases": {
                "Sharedo": "Geekyants\\Sharedo\\SharedoFacade"
            }
        }
    }
}
